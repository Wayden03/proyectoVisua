source('setup.r') #para agarrar las librerias
data <- data.frame(
category = c("Category A", "Category B", "Category C"),
value = c(30, 50, 20)
)
# Create the cake bar chart
ggplot(data, aes(x = "", y = value, fill = category)) +
geom_col(width = 1, color = "white") +
coord_polar(theta = "y") +
theme_void() +
theme(legend.position = "bottom") +
labs(fill = "Category")
View(data)
